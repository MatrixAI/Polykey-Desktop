  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/MatrixAI/PolyKey/issues"
  },
  "build": {
    "electronVersion": "9.0.0",
    "appId": "com.matrixai.polykey",
    "directories": {
      "buildResources": "assets"
    },
    "linux": {
      "target": "tar.gz"
    },
    "win": {
      "icon": "static/logo.png",
      "target": "portable"
    },
    "mac": {
      "icon": "static/logo.png",
      "target": "tar.gz"
    }
  },
  "contributors": [
    "Aaashwin Varshney",
