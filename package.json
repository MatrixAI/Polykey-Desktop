    "google-auth-library": "^6.0.5",
    "js-polykey": "0.0.16",
    "protobufjs": "^6.10.1",
    "readable-stream": "^3.6.0",
    "redux": "^4.0.5",
    "redux-vuex": "^0.3.2",
    "source-map-support": "^0.5.19",
